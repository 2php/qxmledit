<?xml version='1.0'?>
<xs:schema elementFormDefault="qualified" xmlns="http://maven.apache.org/POM/4.0.0" targetNamespace="http://maven.apache.org/POM/4.0.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:annotation/>

       <xs:complexType name="ciao">
