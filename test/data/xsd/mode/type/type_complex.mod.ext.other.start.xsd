             <xs:restriction base="test" >
                 <xs:attribute name="x"/>
             </xs:restriction>
             </xs:complexContent>
             <xs:choice>
                 <xs:element name="x"/>
             </xs:choice>
             <test />
       </xs:complexType>

<xs:complexType name="Parent">
